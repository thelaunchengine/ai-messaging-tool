# Database Configuration
DATABASE_URL="postgresql://username:password@ep-xxx-xxx-xxx.region.aws.neon.tech/database?sslmode=require"

# NextAuth Configuration
NEXTAUTH_SECRET="your-nextauth-secret"
NEXTAUTH_URL="https://your-app.vercel.app"

# AI Model Configuration
OPENAI_API_KEY="your-openai-api-key"
GOOGLE_API_KEY="your-google-api-key"
AI_MODEL_PROVIDER="openai" # openai or gemini

# Redis Configuration
REDIS_URL="redis://username:password@host:port/database"

# Python Backend Configuration
PYTHON_BACKEND_URL="https://aimsg-python.onrender.com"
NEXT_PUBLIC_PYTHON_BACKEND_URL="https://aimsg-python.onrender.com"

# File Upload Configuration
MAX_FILE_SIZE="5242880" # 5MB in bytes
ALLOWED_FILE_TYPES="csv,xlsx,xls"

# Scraping Configuration
SCRAPING_DELAY="2" # seconds between requests
MAX_CONCURRENT_SCRAPES="5"

# Email Configuration (for notifications)
SMTP_HOST="smtp.gmail.com"
SMTP_PORT="587"
SMTP_USER="your-email@gmail.com"
SMTP_PASS="your-app-password"

# Security Configuration
CORS_ORIGINS="http://localhost:3000,http://localhost:3001"
RATE_LIMIT_WINDOW="900000" # 15 minutes in milliseconds
RATE_LIMIT_MAX_REQUESTS="100"

# Monitoring Configuration
SENTRY_DSN="your-sentry-dsn"
LOG_LEVEL="info"

# Development Configuration
NODE_ENV="development"
DEBUG="true"

# Frontend URL (for CORS)
FRONTEND_URL="https://your-app.vercel.app" 